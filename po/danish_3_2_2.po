msgctxt "[SET_PERMISSIONS]"
msgid "Set permissions"
msgstr "Indstil tilladelser"

#. Strings from $lang array key [SET_ROLE_PERMISSIONS]
msgctxt "[SET_ROLE_PERMISSIONS]"
msgid "Set role permissions"
msgstr "Se eller tilpas rolletilladelser"

#. Strings from $lang array key [SET_USERS_PERMISSIONS]
msgctxt "[SET_USERS_PERMISSIONS]"
msgid "Set user permissions"
msgstr "Se eller tilpas brugertilladelser"

#. Strings from $lang array key [SET_USERS_FORUM_PERMISSIONS]
msgctxt "[SET_USERS_FORUM_PERMISSIONS]"
msgid "Set user forum permissions"
msgstr "Se eller tilpas forumtilladelser for brugere"

#. Strings from $lang array key [TRACE_DEFAULT]
msgctxt "[TRACE_DEFAULT]"
msgid "By default every permission is <strong>NO</strong> (unset). So the permission can be overwritten by other settings."
msgstr "Standard er alle tilladelser sat til <strong>NEJ</strong> (ikke sat), så tilladelsen kan tilsidesættes af andre indstillinger."

#. Strings from $lang array key [TRACE_FOR]
msgctxt "[TRACE_FOR]"
msgid "Trace for"
msgstr "Undersøg for"

#. Strings from $lang array key [TRACE_GLOBAL_SETTING]
msgctxt "[TRACE_GLOBAL_SETTING]"
msgid "%s (global)"
msgstr "%s (global)"

#. Strings from $lang array key [TRACE_GROUP_NEVER_TOTAL_NEVER]
msgctxt "[TRACE_GROUP_NEVER_TOTAL_NEVER]"
msgid "This group’s permission is set to <strong>NEVER</strong> like the total result so the old result is kept."
msgstr "Denne gruppes tilladelse er sat til <strong>ALDRIG</strong> som det totale resultat, så det gamle resultat bibeholdes."

#. Strings from $lang array key [TRACE_GROUP_NEVER_TOTAL_NEVER_LOCAL]
msgctxt "[TRACE_GROUP_NEVER_TOTAL_NEVER_LOCAL]"
msgid "This group’s permission for this forum is set to <strong>NEVER</strong> like the total result so the old result is kept."
msgstr "Denne gruppes tilladelse for dette forum er sat til <strong>ALDRIG</strong> som det totale resultat, så det gamle resultat bibeholdes."

#. Strings from $lang array key [TRACE_GROUP_NEVER_TOTAL_NO]
msgctxt "[TRACE_GROUP_NEVER_TOTAL_NO]"
msgid "This group’s permission is set to <strong>NEVER</strong> which becomes the new total value because it wasn’t set yet (set to <strong>NO</strong>)."
msgstr "Denne gruppes tilladelse er sat til <strong>ALDRIG</strong> hvilket bliver den nye totalværdi da det endnu ikke var sat (sat til <strong>NEJ</strong>)."

#. Strings from $lang array key [TRACE_GROUP_NEVER_TOTAL_NO_LOCAL]
msgctxt "[TRACE_GROUP_NEVER_TOTAL_NO_LOCAL]"
msgid "This group’s permission for this forum is set to <strong>NEVER</strong> which becomes the new total value because it wasn’t set yet (set to <strong>NO</strong>)."
msgstr "Denne gruppes tilladelse for dette forum er sat til <strong>ALDRIG</strong> hvilket bliver den nye totalværdi da det endnu ikke var sat (sat til <strong>NEJ</strong>)."

#. Strings from $lang array key [TRACE_GROUP_NEVER_TOTAL_YES]
msgctxt "[TRACE_GROUP_NEVER_TOTAL_YES]"
msgid "This group’s permission is set to <strong>NEVER</strong> which overwrites the total <strong>YES</strong> to a <strong>NEVER</strong> for this user."
msgstr "Denne gruppes tilladelse er sat til <strong>ALDRIG</strong> hvilket tilsidesætter det totale <strong>JA</strong> til et <strong>ALDRIG</strong> for denne bruger."

#. Strings from $lang array key [TRACE_GROUP_NEVER_TOTAL_YES_LOCAL]
msgctxt "[TRACE_GROUP_NEVER_TOTAL_YES_LOCAL]"
msgid "This group’s permission for this forum is set to <strong>NEVER</strong> which overwrites the total <strong>YES</strong> to a <strong>NEVER</strong> for this user."
msgstr "Denne gruppes tilladelse for dette forum er sat til <strong>ALDRIG</strong> hvilket tilsidesætter det totale <strong>JA</strong> til et<strong>ALDRIG</strong> for denne bruger."

#. Strings from $lang array key [TRACE_GROUP_NO]
msgctxt "[TRACE_GROUP_NO]"
msgid "The permission is <strong>NO</strong> for this group so the old total value is kept."
msgstr "Tilladelsen er <strong>NEJ</strong> for denne gruppe så den gamle totalværdi bibeholdes."

#. Strings from $lang array key [TRACE_GROUP_NO_LOCAL]
msgctxt "[TRACE_GROUP_NO_LOCAL]"
msgid "The permission is <strong>NO</strong> for this group within this forum so the old total value is kept."
msgstr "Tilladelsen er <strong>NEJ</strong> for denne gruppe i dette forum så den gamle totalværdi bibeholdes."

#. Strings from $lang array key [TRACE_GROUP_YES_TOTAL_NEVER]
msgctxt "[TRACE_GROUP_YES_TOTAL_NEVER]"
msgid "This group’s permission is set to <strong>YES</strong> but the total <strong>NEVER</strong> cannot be overwritten."
msgstr "Denne gruppes tilladelse er sat til <strong>JA</strong> men totalen <strong>ALDRIG</strong> kan ikke tilsidesættes."

#. Strings from $lang array key [TRACE_GROUP_YES_TOTAL_NEVER_LOCAL]
msgctxt "[TRACE_GROUP_YES_TOTAL_NEVER_LOCAL]"
msgid "This group’s permission for this forum is set to <strong>YES</strong> but the total <strong>NEVER</strong> cannot be overwritten."
msgstr "Denne gruppes tilladelse i dette forum er sat til <strong>JA</strong> men totalen <strong>ALDRIG</strong> kan ikke tilsidesættes."

#. Strings from $lang array key [TRACE_GROUP_YES_TOTAL_NO]
msgctxt "[TRACE_GROUP_YES_TOTAL_NO]"
msgid "This group’s permission is set to <strong>YES</strong> which becomes the new total value because it wasn’t set yet (set to <strong>NO</strong>)."
msgstr "Denne gruppes tilladelse er sat til <strong>JA</strong> hvilket bliver den nye totalværdi da det endnu ikke var sat (sat til <strong>NEJ</strong>)."

#. Strings from $lang array key [TRACE_GROUP_YES_TOTAL_NO_LOCAL]
msgctxt "[TRACE_GROUP_YES_TOTAL_NO_LOCAL]"
msgid "This group’s permission for this forum is set to <strong>YES</strong> which becomes the new total value because it wasn’t set yet (set to <strong>NO</strong>)."
msgstr "Denne gruppes tilladelse i dette forum er sat til <strong>JA</strong> hvilket bliver den nye totalværdi da det endnu ikke var sat (set til <strong>NEJ</strong>)."

#. Strings from $lang array key [TRACE_GROUP_YES_TOTAL_YES]
msgctxt "[TRACE_GROUP_YES_TOTAL_YES]"
msgid "This group’s permission is set to <strong>YES</strong> and the total permission is already set to <strong>YES</strong>, so the total result is kept."
msgstr "Denne gruppes tilladelse er sat til <strong>JA</strong> og de totale tilladelser er allerede sat til <strong>JA</strong>, så det totale resultat bibeholdes."

#. Strings from $lang array key [TRACE_GROUP_YES_TOTAL_YES_LOCAL]
msgctxt "[TRACE_GROUP_YES_TOTAL_YES_LOCAL]"
msgid "This group’s permission for this forum is set to <strong>YES</strong> and the total permission is already set to <strong>YES</strong>, so the total result is kept."
msgstr "Denne gruppes tilladelse i dette forum er sat til <strong>JA</strong> og de totale tilladelser er allerede sat til <strong>JA</strong>, så det totale resultat bibeholdes."

#. Strings from $lang array key [TRACE_PERMISSION]
msgctxt "[TRACE_PERMISSION]"
msgid "Trace permission - %s"
msgstr "Undersøg tilladelse - %s"

#. Strings from $lang array key [TRACE_RESULT]
msgctxt "[TRACE_RESULT]"
